# Salin file ini menjadi `.env` dan isi kredensial Anda secara lokal.
# Jangan commit `.env` ke repository publik.

# Cookie Facebook untuk facebook_scraper (format dict JSON).
FACEBOOK_COOKIES={"c_user":"", "xs":""}

# API key optional untuk Google Custom Search (page discovery).
GOOGLE_API_KEY=
GOOGLE_CSE_ID=

# API key untuk Perspective API atau layanan penilaian toksisitas lainnya (opsional).
PERSPECTIVE_API_KEY=

# API key untuk FastAPI (digunakan oleh api/app.py).
API_KEY=

# HuggingFace token jika ingin mengakses model privat.
HUGGINGFACE_TOKEN=
